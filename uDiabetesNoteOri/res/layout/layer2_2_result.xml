<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">

<!-- ###################TOP PANEL########################## -->
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@drawable/sub_title"
		android:id="@+id/relativeLayout3">

		<Button android:text=""
			android:id="@+id/button_layer3_back"
			android:background="@drawable/prev_selector"
			android:layout_centerVertical="true"
			android:layout_marginLeft="16px"
			android:layout_marginTop="30px"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"></Button>

		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:gravity="center_horizontal"
		    android:text="비만도 결과"
		    android:textColor="#ffffff"
		    android:textSize="18dp"
		    android:textStyle="bold" />
	</RelativeLayout>

<!-- ###################CONTENT########################## -->	
	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	   android:background="#FFFFFF"
	    android:orientation="vertical" >

<!-- ###################이상체중########################## -->			
	<WebView 
	    android:layout_margin="5dp"
	    android:id="@+id/webview"    
	    android:layout_width="fill_parent"        
	    android:layout_height="wrap_content" />

<!-- ###################표준체중계산법########################## -->		
	<RelativeLayout
	    android:layout_marginLeft="5dp"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content" >

		<ImageView
		    android:id="@+id/imageView1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:src="@drawable/arrow01" />

		<TextView
		    android:id="@+id/textView1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_toRightOf="@+id/imageView1"
		    android:text="표준체중 계산법"
		    android:textColor="#000000"
		    android:textSize="17dp" />

	</RelativeLayout>
	
	<WebView 
	    android:id="@+id/webview2"    
	    android:layout_width="fill_parent"        
	    android:layout_height="wrap_content" />

<!-- ###################비만도########################## -->				
	<RelativeLayout
	    android:layout_marginLeft="5dp"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content" >

		<ImageView
		    android:id="@+id/imageView1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:src="@drawable/arrow01" />

		<TextView
		    android:id="@+id/textView2"    
		    android:layout_toRightOf="@+id/imageView1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textColor="#000000"
		    android:textSize="17dp"
		    android:text="비만도" />

	</RelativeLayout>
	
	<LinearLayout
	     android:layout_marginLeft="5dp"
		 android:layout_width="wrap_content"
		 android:layout_height="wrap_content"
		 android:layout_alignLeft="@+id/textView2"
		 android:layout_below="@+id/imageView1"
		 android:orientation="vertical" >
		 
	    <TextView
		    android:id="@+id/textView3"
		    android:layout_marginTop="1dp"    
		    android:layout_toRightOf="@+id/imageView1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textColor="#000000"
		    android:textSize="11dp"
		    android:text="비만도(%)=현재체중(kg)/표준체중(kg)*100" />
	    
	</LinearLayout>	

	<TableLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content" 
	    android:layout_margin="6dp"
	    android:stretchColumns="0,2">

	    <TableRow
	        android:id="@+id/tableRow1"
	        android:background="#8FDAFF"	      
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >

	        <TextView
	            android:id="@+id/textView3"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"               
	            android:layout_gravity="center_horizontal"
	            android:text="구 분" />
	        
	        <TextView
	            android:id="@+id/textView4"
	            android:layout_width="1dp"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
				android:background="#FFFFFF"	      	            
	            android:text="" />	        
	        
	        <TextView
	            android:id="@+id/textView5"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
	            android:layout_gravity="center_horizontal"				      
	            android:text="비만도 범주" />	        
	        
	    </TableRow>

<!-- ###################안쪽선########################## -->					    
	    <TableRow
	        android:id="@+id/tableRow2"
	        android:background="#FFA500"	      
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >
        
	        <TextView
	            android:id="@+id/textView6"
	            android:layout_span="3"	            
	            android:layout_height="1dp"
	            android:textColor="#000000"
				android:background="#FFFFFF"	      	            
	            android:text="" />	     
	     	        	   	        
	    </TableRow>
	    
		<TableRow
	        android:id="@+id/tableRow3"
	        android:background="#DEFFFF"	      
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >

	        <TextView
	            android:id="@+id/textView7"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"               
	            android:layout_gravity="center_horizontal"
	            android:text="저 체 중" />
	        
	        <TextView
	            android:id="@+id/textView8"
	            android:layout_width="1dp"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
				android:background="#FFFFFF"	      	            
	            android:text="" />	        
	        
	        <TextView
	            android:id="@+id/textView9"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
	            android:layout_gravity="center_horizontal"				      
	            android:text="90% 이하" />	        
	        
	    </TableRow>	    

<!-- ###################안쪽선########################## -->					    
	    <TableRow
	        android:id="@+id/tableRow4"
	        android:background="#FFA500"	      
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >
        
	        <TextView
	            android:id="@+id/textView10"
	            android:layout_span="3"	            
	            android:layout_height="1dp"
	            android:textColor="#000000"
				android:background="#FFFFFF"	      	            
	            android:text="" />	     
	     	        	   	        
	    </TableRow>
	    
		<TableRow
	        android:id="@+id/tableRow5"
	        android:background="#DEFFFF"	      
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >

	        <TextView
	            android:id="@+id/textView11"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"               
	            android:layout_gravity="center_horizontal"
	            android:text="정 상" />
	        
	        <TextView
	            android:id="@+id/textView12"
	            android:layout_width="1dp"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
				android:background="#FFFFFF"	      	            
	            android:text="" />	        
	        
	        <TextView
	            android:id="@+id/textView13"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
	            android:layout_gravity="center_horizontal"				      
	            android:text="90 ~ 100%" />	        
	        
	    </TableRow>	    
	    
<!-- ###################안쪽선########################## -->					    
	    <TableRow
	        android:id="@+id/tableRow6"
	        android:background="#FFA500"	      
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >
        
	        <TextView
	            android:id="@+id/textView14"
	            android:layout_span="3"	            
	            android:layout_height="1dp"
	            android:textColor="#000000"
				android:background="#FFFFFF"	      	            
	            android:text="" />	     
	     	        	   	        
	    </TableRow>		
	    
		<TableRow
	        androidid="@+id/tableRow7"
	        android:background="#FFEEFF"	      
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >

	        <TextView
	            android:id="@+id/textView15"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"               
	            android:layout_gravity="center_horizontal"
	            android:text="과 체 중" />
	        
	        <TextView
	            android:id="@+id/textView16"
	            android:layout_width="1dp"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
				android:background="#FFFFFF"	      	            
	            android:text="" />	        
	        
	        <TextView
	            android:id="@+id/textView17"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
	            android:layout_gravity="center_horizontal"				      
	            android:text="111 ~ 120%" />	        
	        
	    </TableRow>		
	    
<!-- ###################안쪽선########################## -->					    
	    <TableRow
	        android:id="@+id/tableRow8"
	        android:background="#FFA500"	      
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >
        
	        <TextView
	            android:id="@+id/textView18"
	            android:layout_span="3"	            
	            android:layout_height="1dp"
	            android:textColor="#000000"
				android:background="#FFFFFF"	      	            
	            android:text="" />	     
	     	        	   	        
	    </TableRow>		
	    
		<TableRow
	        androidid="@+id/tableRow9"
	        android:background="#FFEEFF"	      
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" >

	        <TextView
	            android:id="@+id/textView19"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"               
	            android:layout_gravity="center_horizontal"
	            android:text="비 만" />
	        
	        <TextView
	            android:id="@+id/textView20"
	            android:layout_width="1dp"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
				android:background="#FFFFFF"	      	            
	            android:text="" />	        
	        
	        <TextView
	            android:id="@+id/textView21"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:textColor="#000000"
	            android:layout_gravity="center_horizontal"				      
	            android:text="121% 이상" />	        
	        
	    </TableRow>					

	</TableLayout>
	    
	</LinearLayout>

</LinearLayout>


