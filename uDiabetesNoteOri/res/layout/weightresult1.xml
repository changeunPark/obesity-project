<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="200dip">

<!-- 
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="80px"
		android:background="@drawable/sub_title"
		android:id="@+id/relativeLayout2_1_result">

		<TextView android:textColor="#000000"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:gravity="center_horizontal"
			android:textSize="18dip"
			android:textStyle="bold"
			android:text="비만도 검사" />

	</RelativeLayout>

 -->
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_below="@+id/relativeLayout2_1_result"
		android:layout_height="wrap_content"
		android:background="@drawable/sub_bg"
		android:id="@+id/relativeLayout21_result">

		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="18px"
			android:background="@drawable/outborder"
			android:id="@+id/relativeLayout2_1_1_result">

			<RelativeLayout
				android:background="@drawable/border"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="15dip"
					android:layout_marginBottom="15dip"
					android:id="@+id/linearLayout_2_1_1_result">

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dip"
						android:textSize="18dp"
						android:textColor="#000000"
						android:id="@+id/textview_2_1_height_result"
						android:text="이상체중: " />

					<TextView
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="50dip"
						android:layout_marginRight="10dip"
						android:textSize="18dp"
						android:textColor="#0000FF"
						android:layout_toRightOf="@+id/textview_2_1_height_result"
						android:id="@+id/textview_2_1_height_result_1"
						android:text="kg" />

				</LinearLayout>


				<RelativeLayout
					android:id="@+id/rlayout0000_2_1_result"
					android:layout_below="@+id/linearLayout_2_1_1_result"
					android:layout_width="fill_parent"
					android:background="#DDDDDD"
					android:layout_height="2dip"></RelativeLayout>

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="15dip"
					android:layout_below="@+id/rlayout0000_2_1_result"
					android:id="@+id/linearLayout_2_2_result">

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:textColor="#000000"
						android:textSize="18dp"
						android:layout_marginLeft="10dip"
						android:layout_marginBottom="15dip"
						android:id="@+id/textview_2_1_weight_result"
						android:text="비만도: " />
					<TextView
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="50dip"
						android:layout_marginRight="10dip"
						android:textSize="18dp"
						android:textColor="#0000FF"
						android:layout_toRightOf="@+id/textview_2_1_weight_result"
						android:id="@+id/textview_2_1_weight_result_1"
						android:text="" />

				</LinearLayout>


			</RelativeLayout>
		</RelativeLayout>

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="25px"
			android:layout_below="@+id/relativeLayout2_1_1_result"
			android:gravity="center_horizontal"
			android:id="@+id/linearLayout_2_1_1_result">

			<Button android:text=""
				android:background="@drawable/button_confirm_general_selector"
				android:id="@+id/button_2_1_result_result"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"></Button>

		</LinearLayout>

	</RelativeLayout>

</LinearLayout>
