						android:textColor="@color/mred"
						android:textSize="18dp"
						android:layout_toRightOf="@+id/textview_2_2_height_result"
						android:id="@+id/textview_2_2_height_result_1"
						android:text="kg" />

				</LinearLayout>


				<RelativeLayout
					android:id="@+id/rlayout0000_2_2_result"
					android:layout_below="@+id/linearLayout_2_2_1_result"
