				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"	
					android:layout_below="@+id/rlayout0000_2_2_result"
					android:id="@+id/linearLayout_2_2_2_result">

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:textColor="#000000"
						android:layout_marginLeft="10dip"
						android:textSize="18dp"
						android:id="@+id/textview_2_2_weight_result"
						android:text=" 체중 상태      :" />
					<TextView
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="50dip"
						android:layout_marginRight="10dip"
						android:textColor="@color/mred"
						android:textSize="18dp"
						android:layout_toRightOf="@+id/textview_2_2_weight_result"
						android:id="@+id/textview_2_2_weight_result_1"
						android:text="" />

				</LinearLayout>


			</RelativeLayout>
		</RelativeLayout>


	</RelativeLayout>

</LinearLayout>

