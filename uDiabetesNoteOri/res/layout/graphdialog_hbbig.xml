	<RelativeLayout
	    android:layout_marginTop="10dp"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">
		
	 <LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center_horizontal"
		android:id="@+id/linearLayout_title">

		<TextView
		    android:id="@+id/textview_graph_1"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="10dip"
		    android:textColor="@color/white"
		    android:textSize="18dp"
		    android:textStyle="bold" />

	</LinearLayout>
		
	<LinearLayout
	    android:layout_below="@+id/linearLayout_title"
	    android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:gravity="center_horizontal"
		android:layout_marginTop="15dip"
		android:layout_marginBottom="10dip"
		android:id="@+id/linearLayout_graph_2_1_result_1">

		<TextView
		    android:id="@+id/textview_graph_1"
		    android:layout_width="wrap_content"
		    android:layout_height="fill_parent"
		    android:layout_marginLeft="10dip"
		    android:gravity="center"
		    android:text="기간 "
		    android:textColor="@color/white"
		    android:textSize="18dp" />

		<Button
		    android:id="@+id/button1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:background="@drawable/btn_datebg"
		    android:text="Button" />
		
		<TextView
		    android:id="@+id/textview"
		    android:layout_width="wrap_content"
		    android:layout_height="fill_parent"
		    android:gravity="center"
		    android:text=" ~ "
		    android:textColor="@color/white"
		    android:textSize="18dp" />

		<!--  
		<TextView
		    android:id="@+id/textview_graph_2"
		    android:layout_height="wrap_content"
		    android:layout_width="wrap_content"
		    android:layout_marginLeft="10dip"
		    android:text="2011/11/01"
		    android:textColor="@color/ltgrey"
		    android:textSize="18dp" />
		-->

		<Button
		    android:id="@+id/button2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:background="@drawable/btn_datebg"
		    android:text="Button" />
		
		<Button
		    android:id="@+id/btn_search"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="5dp"
		    android:background="@drawable/button_search_selector" />

	</LinearLayout>
		
	    
	<LinearLayout
	    android:layout_below="@+id/linearLayout_graph_2_1_result_1"
		android:layout_marginTop="1dp"
		android:layout_width="100dp"
		android:id="@+id/linear_line"
		android:layout_height="400dp">		
	</LinearLayout>

	<HorizontalScrollView
	    android:layout_below="@+id/linearLayout_graph_2_1_result_1"
			android:id="@+id/horizontalScrollView1_graph"
			android:layout_marginTop="1dp"
			android:layout_marginLeft="-65dp"
			android:layout_toRightOf="@+id/linear_line"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content">
			
			<LinearLayout
				android:id="@+id/linearLayout1_graph"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="horizontal">
				
				<LinearLayout
					android:layout_width="6100dp"
					android:layout_height="400dp"
					android:id="@+id/linear_graphdialog_2222">
				</LinearLayout>

			</LinearLayout>
	</HorizontalScrollView>

	</RelativeLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="1dp"
		android:layout_marginBottom="10dp"
		android:gravity="right"
		android:id="@+id/linearLayout_3_2_2_graphdialog_1">
		
	<TextView
	    android:id="@+id/textStandard"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginRight="5dp"
	    android:text="*목표 혈당범위 : 80~150"
	    android:textColor="#FFFFFFFF"
	    android:textSize="12dp" />
	    	   	
	</LinearLayout>
	
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="15px"
		android:layout_marginBottom="10dp"
		android:gravity="center_horizontal"
		android:id="@+id/linearLayout_3_2_2_graphdialog_1">
		
	    <!-- 	
	    <Button android:text=""		
			android:background="@drawable/button_select_selector"
			android:id="@+id/button_graph"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"></Button>
	     -->	     
	     
	    <Button android:text=""
	        android:layout_marginLeft="10dip"
			android:background="@drawable/button_confirm_general_selector"
			android:id="@+id/button_3_2_graphdialog_result"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"></Button>
	    	   	
	</LinearLayout>

</LinearLayout>