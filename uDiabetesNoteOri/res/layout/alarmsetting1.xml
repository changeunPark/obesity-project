<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content">


	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#FFFFFF"
		android:id="@+id/relativeLayout_config_5_1">


		<RadioGroup
			android:id="@+id/radiogroup_config_5_1"
			android:layout_width="fill_parent"
			android:orientation="vertical"
			android:layout_height="wrap_content">



			<RadioButton android:id="@+id/radio_config_5_1"
				android:layout_width="wrap_content"
				android:textColor="#000000"
				android:layout_marginRight="10dip"
				android:layout_marginLeft="10dip"
				android:layout_alignParentRight="true"
				android:layout_height="wrap_content"
				android:text="                                                   기본"></RadioButton>



			<RelativeLayout
				android:id="@+id/rlayout0000_config_5_1"
				android:layout_below="@+id/relativeLayout_config_5_2"
				android:layout_width="fill_parent"
				android:background="#DDDDDD"
				android:layout_height="2dip"></RelativeLayout>



			<RadioButton android:id="@+id/radio_config_5_2"
				android:layout_width="wrap_content"
				android:textColor="#000000"
				android:layout_marginRight="10dip"
				android:layout_marginLeft="10dip"
				android:layout_alignParentRight="true"
				android:layout_height="wrap_content"
				android:text="                                                   진동"></RadioButton>



			<RelativeLayout
				android:id="@+id/rlayout0000_config_5_2"
				android:layout_below="@+id/relativeLayout_config_5_3"
				android:layout_width="fill_parent"
				android:background="#DDDDDD"
				android:layout_height="2dip"></RelativeLayout>



			<RadioButton android:id="@+id/radio_config_5_3"
				android:layout_width="wrap_content"
				android:textColor="#000000"
				android:layout_marginRight="10dip"
				android:layout_marginLeft="10dip"
				android:layout_alignParentRight="true"
				android:layout_height="wrap_content"
				android:text="                                                   무음"></RadioButton>


		</RadioGroup>

	</RelativeLayout>


	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="25px"
		android:layout_below="@+id/relativeLayout_config_5_1"
		android:gravity="center_horizontal"
		android:id="@+id/linearLayout_config_5_1">

		<Button android:text="취소"
			android:id="@+id/button_config_5_1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"></Button>

	</LinearLayout>


</LinearLayout>
