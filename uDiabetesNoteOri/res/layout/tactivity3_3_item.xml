<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/linearLayout__5_input_1111_1_title"
        android:background="@color/mblue"
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/textView_5_title123_1_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_margin="10dp"
            android:text="혈당리스트"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:textStyle="bold" >
        </TextView>
    </LinearLayout>

    <ScrollView
        android:id="@+id/scrollview_exeinput123_1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout__5_input_1111_1_title" >

        <LinearLayout
            android:id="@+id/linearLayout_exeinput123_1_1_1_1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="60dp" >

            <RelativeLayout
                android:id="@+id/relativeLayout51_input123"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView_5_input123_1_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="0dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:padding="5dp"
                    android:text="혈당수치"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#000000"
                    android:textSize="18dp" >
                </TextView>

                <EditText
                    android:id="@+id/textView_5_input123_1_2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView_5_input123_1_1"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:inputType="number"
                    android:padding="5dp"
                    android:singleLine="true"
                    android:text=""
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#000000"
                    android:textSize="18dp" >
                </EditText>

                <TextView
                    android:id="@+id/textView_5_input123_1_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView_5_input123_1_2"
                    android:layout_marginLeft="10dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:padding="5dp"
                    android:text="메모"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#000000"
                    android:textSize="18dp" >
                </TextView>

                <EditText
                    android:id="@+id/textView_5_input123_1_5"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView_5_input123_1_4"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:lines="1"
                    android:padding="5dp"
                    android:scrollbars="vertical"
                    android:text=""
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#000000"
                    android:textSize="18dp" >
                </EditText>

                <!-- <TextView -->
                <!-- android:gravity="center_horizontal|center_vertical" -->
                <!-- android:textColor="#000000" -->
                <!-- android:padding="5dp" -->
                <!-- android:textSize="18dp" -->
                <!-- android:layout_marginLeft="10dp" -->
                <!-- android:id="@+id/textView_5_input123_1_6" -->
                <!-- android:layout_width="wrap_content" -->
                <!-- android:layout_below="@+id/textView_5_input123_1_5" -->
                <!-- android:layout_height="wrap_content" -->
                <!-- android:textAppearance="?android:attr/textAppearanceMedium" -->
                <!-- android:text="시간"></TextView> -->


                <!-- <TimePicker -->
                <!-- android:id="@+id/timePicker_input123" -->
                <!-- android:layout_width="wrap_content" -->
                <!-- android:layout_height="wrap_content" -->
                <!-- android:layout_below="@+id/textView_5_input123_1_6" -->
                <!-- android:layout_marginBottom="20dp" -->
                <!-- android:layout_marginLeft="10dp" -->
                <!-- android:layout_marginTop="0dp" > -->


                <!-- </TimePicker> -->

            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/linearLayout__5_input_1111_1_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/scrollview_exeinput123_1"
        android:gravity="center_horizontal|center_vertical" >

        <Button
            android:id="@+id/button__5_input123_1_result_update"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="수정" >
        </Button>

        <Button
            android:id="@+id/button__5_input123_1_result_delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="제거" >
        </Button>
    </LinearLayout>

</RelativeLayout>
